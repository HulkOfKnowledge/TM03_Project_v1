{
  "name": "creduman",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:python\" --names \"next,python\" --prefix-colors \"cyan,magenta\"",
    "dev:python": "cd credit-intelligence-service && ./venv/bin/python main.py",
    "build": "next build",
    "start": "next start",
    "start:all": "concurrently \"npm run start\" \"npm run start:python\" --names \"next,python\" --prefix-colors \"cyan,magenta\"",
    "start:python": "cd credit-intelligence-service && ./venv/bin/python main.py",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "supabase migration up",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "ml:train": "cd credit-intelligence-service && ./venv/bin/python app/ml/train.py",
    "ml:test": "cd credit-intelligence-service && ./venv/bin/python app/test_service.py"
  },
  "dependencies": {
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.43.0",
    "axios": "^1.7.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.563.0",
    "next": "^14.2.0",
    "next-intl": "^3.15.0",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.11.0",
    "typescript": "^5.4.0"
  }
}
